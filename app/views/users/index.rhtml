<h1>Listing users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>E-mail</th>
    <th>Username</th>
    <th>Password</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.username %></td>
    <td><%= user.hashed_password %></td>
  </tr>
<% end %>
</table>
